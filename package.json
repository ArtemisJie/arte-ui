    "classnames": "^2.3.2",
    "loadsh": "^0.0.4",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.59.3",
    "save": "^2.9.0",
    "scss": "^0.2.4",
    "vite-plugin-react": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": ">=7.22.0 <8.0.0",
    "@babel/plugin-syntax-flow": "^7.14.5",
